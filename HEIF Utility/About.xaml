<Window x:Class="HEIF_Utility_HiDPI.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HEIF_Utility_HiDPI"
        mc:Ignorable="d"
        Title="{DynamicResource More.About}" Height="200" Width="318" ResizeMode="NoResize" ShowInTaskbar="False" Topmost="True">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock Margin="4,0,0,0" Text="{DynamicResource ProgramName}" FontSize="18" Foreground="#518ffc"/>
    </Grid>
</Window>
